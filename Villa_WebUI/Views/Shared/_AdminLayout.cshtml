
@{
    Layout = null;
}

<!DOCTYPE html>

<html lang="en">
    <head>
       @await Component.InvokeAsync("_AdminHead")
    </head>

<body id="body" >

    <!--*******************
        Preloader start
    ********************-->
    @await Component.InvokeAsync("_AdminPreloader")
    <!--*******************
        Preloader end
    ********************-->


    <!--**********************************
        Main wrapper start
    ***********************************-->
    <div id="main-wrapper">

        <!--**********************************
            Logo Seciton
        ***********************************-->
        @await Component.InvokeAsync("_AdminLogo")
        <!--**********************************
            End Logo Section
        ***********************************-->

        <!--**********************************
            Header start
        ***********************************-->
        @await Component.InvokeAsync("_AdminNavbar")
        <!--**********************************
            Header end ti-comment-alt
        ***********************************-->

        <!--**********************************
            Sidebar start
        ***********************************-->
        @await Component.InvokeAsync("_AdminSidebar")
        <!--**********************************
            Sidebar end
        ***********************************-->

        <!--**********************************
            Content body start
        ***********************************-->
        <div class="content-body">
            <div class="container-fluid">
                <div class="row page-titles mx-0">
                    <div class="col-sm-6 p-md-0">
                        <div class="welcome-text">
                            <h4>@ViewData["Title"]</h4>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="card">
                            
                            <div class="card-body">
                                @RenderBody()
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--**********************************
            Content body end
        ***********************************-->


        <!--**********************************
            Footer start
        ***********************************-->
        @await Component.InvokeAsync("_AdminFooter")
        <!--**********************************
            Footer end
        ***********************************-->

    </div>

    @await Component.InvokeAsync("_AdminScripts")
    <!--**********************************
        Scripts
    ***********************************-->
    <!-- Required vendors -->
    
    <!-- Circle progress -->




</body>

</html>
